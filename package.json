{
  "name": "strapi-plugin-do-not-delete",
  "version": "1.0.0",
  "description": "A plugin for Strapi CMS that protects certain entries from being deleted.",
  "license": "MIT",
  "strapi": {
    "displayName": "Do Not Delete",
    "name": "do-not-delete",
    "description": "Protects certain entries from being deleted.",
    "kind": "plugin"
  },
  "author": {
    "name": "Matt Milburn",
    "url": "https://github.com/mattmilburn"
  },
  "maintainers": [
    {
      "name": "Matt Milburn",
      "url": "https://github.com/mattmilburn"
    }
  ],
  "engines": {
    "node": ">=14.19.1 <=18.x.x",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --fix-dry-run --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "format": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "test": "jest --testPathPattern='\\/__tests__\\/.*\\.(?:test|spec)\\.js$' --ci  2>&1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --check",
      "eslint --fix-dry-run"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/eslint-parser": "7.22.9",
    "@babel/eslint-plugin": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "eslint": "8.45.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "jest-junit": "16.0.0",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8"
  },
  "peerDependencies": {
    "@strapi/strapi": "^4.9.0",
    "@strapi/utils": "^4.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattmilburn/strapi-plugin-do-not-delete.git"
  },
  "keywords": [
    "strapi",
    "plugin",
    "dont",
    "don't",
    "do",
    "not",
    "delete",
    "destroy",
    "remove"
  ]
}
